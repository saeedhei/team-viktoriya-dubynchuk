{
  "name": "upload-photos",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "check-node": "tsx scripts/check-node-version.ts",
    "check-env": "tsx scripts/checkEnv.ts",
    "lint": "eslint . --fix",
    "dev": "npm run check-node && npm run check-env && cross-env NODE_ENV=development nodemon",
    "test": "cross-env NODE_ENV=test jest",
    "test:unit": "cross-env NODE_ENV=test jest --testPathPattern=unit",
    "test:integration": "cross-env NODE_ENV=test jest --testPathPattern=integration",
    "test:watch": "cross-env NODE_ENV=test jest --watch",
    "test:coverage": "cross-env NODE_ENV=test jest --coverage",
    "build": "tsc",
    "postbuild": "ts-node scripts/copy-files.ts",
    "serve": "cross-env NODE_ENV=production node dist/bin/www.js"
  },
  "dependencies": {
    "@apollo/server": "^4.11.3",
    "@graphql-tools/merge": "^9.0.24",
    "axios": "^1.8.1",
    "cookie-parser": "~1.4.7",
    "cors": "^2.8.5",
    "debug": "~4.4.0",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "graphql": "^16.10.0",
    "graphql-tag": "^2.12.6",
    "http-errors": "~2.0.0",
    "mongoose": "^8.12.1",
    "morgan": "~1.10.0",
    "nano": "^10.1.4",
    "pug": "3.0.3"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.8",
    "@types/cors": "^2.8.17",
    "@types/debug": "^4.1.12",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.13.9",
    "@types/semver": "^7.5.8",
    "@types/supertest": "^6.0.2",
    "@typescript-eslint/eslint-plugin": "^8.26.0",
    "@typescript-eslint/parser": "^8.26.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.21.0",
    "jest": "^29.7.0",
    "mongodb-memory-server": "^10.1.4",
    "nodemon": "^3.1.9",
    "prettier": "^3.5.3",
    "semver": "^7.7.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.6",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.3",
    "typescript": "^5.8.2"
  }
}
